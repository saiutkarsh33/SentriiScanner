{
  "manifest_version": 3,
  "name": "Smart Contract Scanner",
  "version": "1.0",
  "description": "Scan Smart Contract Addresses",
  "permissions": ["activeTab", "storage", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "16x16.png",
      "48": "48x48.png",
      "128": "128x128.png"
    }
  }
}
